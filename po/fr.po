#, c-format
msgid "letters: %s\n"
msgstr "Lettres : %s\n"

#: dic/header.cpp:442
#, c-format
msgid "number of letters: %lu\n"
msgstr "Nombre de lettres : %lu\n"

#: dic/header.cpp:443
#, c-format
msgid "number of words: %d\n"
msgstr "Nombre de mots : %d\n"

#: dic/header.cpp:445
#, c-format
msgid "header size: %lu bytes\n"
msgstr "Taille du header : %lu octets\n"

#: dic/header.cpp:446
#, c-format
msgid "root: %d (edge)\n"
msgstr "Racine : %d (arcs)\n"

